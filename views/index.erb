<!DOCTYPE html>
<html lang="ja">
<head>
    <meta charset="UTF-8">
	<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="/application.js" type="text/javascript"></script>
    <title>チャット</title>
</head>
<body>
    <form id="form">
        <input type="text"name="id"id="id"  placeholder="id">
        <input type="text"name="x"id="x" placeholder="x">
        <input type="text"name="y"id="y" placeholder="y">
        <input type="text"name="angle"id="angle"  placeholder="angle">
        <input type="text"name="power"id="power" placeholder="power">
        <input type="text" id="send-msg">
        <input type="submit" id="sbm">
    </form>
    <ul id="msgs"></ul>
</body>
<script>
$(document).on("click", "#sbm", function() {
    var id = $("#id").val();
    var x = $("#x").val();
    var y = $("#y").val();
    var angle = $("#angle").val();
    var power = $("#power").val();

    $.ajax({
        url: "/eraser/position",
        type: "POST",
        data: {
            id: id,
            x: x,
            y: y,
            angle: angle,
            power: power,
        },
        success: function(json){
            // $('ul').append('<li>'+ json.ok +'</li>');
            // $('ul').append('<li>'+ json.data.id +'</li>');
            // $('ul').append('<li>'+ json.data.event +'</li>');
            // $('ul').append('<li>'+ json.data.angle +'</li>');
            // $('ul').append('<li>'+ json.data.power +'</li>');

            console.log("OK");

        },
        error: function() {
            console.log("NG");
        }
    });
});
</script>
</html>